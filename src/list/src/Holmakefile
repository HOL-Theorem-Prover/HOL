EXTRA_CLEANS = selftest.exe list-selftest.log

all: $(DEFAULT_TARGETS) selftest.exe
.PHONY: all

selftest.exe: selftest.uo rich_listTheory.uo
	$(HOLMOSMLC) -o $@ $<

ifeq($(KERNELID),otknl)

