CLINE_OPTIONS=-j1

default: LassieTestTheory.sml

sempre:
	cd ../sempre &&\
	rm -rf classes/ fig/ int-output/ interactive/lassie.lexicon interactive/sempre-out-socket.sml lib/ libsempre/ module-classes.txt state/ &&\
	./pull-dependencies core interactive &&\
	ant core interactive &&\
	cd ../src
.PHONY: sempre

LassieTestTheory.sml: sempre

all: $(DEFAULT_TARGETS)
.PHONY: all

sempre_clean: $(clean)
	cd ../sempre &&\
	rm -rf classes/ fig/ int-output/ interactive/lassie.lexicon interactive/sempre-out-socket.sml lib/ libsempre/ module-classes.txt state/ &&\
.PHONY: sempre_clean
