EXTRA_CLEANS = heap \
	$(patsubst %Theory.uo,%Theory.html,$(TARGETS)) \
	$(patsubst %Theory.uo,%Theory.lst,$(TARGETS))

ifdef POLY
HOLHEAP = heap
OBJNAMES = realLib hurdUtils probabilityTheory

OBJS = $(patsubst %,$(dprot $(SIGOBJ)/%.uo),$(OBJNAMES))

all: $(HOLHEAP)

$(HOLHEAP): $(OBJS) $(dprot $(HOLDIR)/bin/hol.state)
	$(protect $(HOLDIR)/bin/buildheap) -o $@ $(OBJNAMES)
endif

all: $(DEFAULT_TARGETS)

.PHONY: all
