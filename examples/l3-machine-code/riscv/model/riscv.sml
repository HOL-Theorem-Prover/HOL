fun archBase a =
  case a of
     RV32I => BitsN.B(0x0,2)
   | RV64I => BitsN.B(0x2,2)
   | RV128I => BitsN.B(0x3,2);

fun architecture ab =
  case ab of
     BitsN.B(0x0,_) => RV32I
   | BitsN.B(0x2,_) => RV64I
   | BitsN.B(0x3,_) => RV128I
   | _ =>
     raise UNDEFINED
       ("Unknown architecture: " ^ (Nat.toString(BitsN.toNat ab)));

fun archName a =
  case a of RV32I => "RV32I" | RV64I => "RV64I" | RV128I => "RV128I";

fun privLevel p =
  case p of
     User => BitsN.B(0x0,2)
   | Supervisor => BitsN.B(0x1,2)
   | Hypervisor => BitsN.B(0x2,2)
   | Machine => BitsN.B(0x3,2);

fun privilege p =
  case p of
     BitsN.B(0x0,_) => User
   | BitsN.B(0x1,_) => Supervisor
   | BitsN.B(0x2,_) => Hypervisor
   | BitsN.B(0x3,_) => Machine
   | _ => raise General.Bind;

fun privName p =
