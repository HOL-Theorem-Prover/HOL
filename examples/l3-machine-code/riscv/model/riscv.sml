fun initRegs pc =
  ( L3.for
      (0,31,
       fn i =>
         let
           val x = BitsN.fromNat(i,5)
         in
           write'gpr(BitsN.B(0x0,64),x)
         end)
  ; L3.for
      (0,31,
       fn i =>
         let
           val x = BitsN.fromNat(i,5)
         in
           write'fpr(BitsN.B(0x0,64),x)
         end)
  ; write'PC(BitsN.fromNat(pc,64))
  ; write'NextFetch NONE
  ; done := false
  );

